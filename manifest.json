{
  "name": "OfferUp Helper",
  "version": "1.0",
  "description": "Helps to remove unlisted and removed items from you boards.",
  "manifest_version": 2,
  "permissions": [
    "webRequest"
  ],
  "content_scripts": [
    {
      "matches": [
        "https://offerup.com/board/*"
      ],
      "js": ["js/jquery-3.4.1.min.js", "js/content.js"]
    }
  ],
  "icons": {
    "16": "images/logo_16x16.png",
    "32": "images/logo_32x32.png",
    "48": "images/logo_48x48.png",
    "128": "images/logo_128x128.png"
  },
  "background": {
    "scripts": ["js/background.js"]
  }
}